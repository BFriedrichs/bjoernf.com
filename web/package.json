{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "homepage": "/public",
  "dependencies": {
    "@mui/icons-material": "^5.8.4",
    "@remirror/pm": "^1.0.22",
    "@remirror/react": "^1.0.43",
    "ackee-tracker": "^5.1.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "preact": "^10.10.3",
    "react-router-dom": "^6.3.0",
    "react-share": "^4.4.0",
    "react-toast": "^1.0.3",
    "remirror": "^1.0.89",
    "styled-components": "^5.3.5"
  },
  "scripts": {
    "start": "parcel watch",
    "build": "rm -rf dist .parcel-cache && parcel build",
    "build:report": "rm -rf dist .parcel-cache && parcel build --reporter @parcel/reporter-bundle-analyzer",
    "postinstall": "patch-package"
  },
  "targets": {
    "default": {
      "source": "app/src/index.html",
      "distDir": "dist/browser",
      "context": "browser",
      "publicUrl": "/static",
      "scopeHoist": true,
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    },
    "node": {
      "source": "app/src/AppServer.js",
      "distDir": "dist/node",
      "context": "node",
      "publicUrl": "/static",
      "outputFormat": "commonjs",
      "includeNodeModules": {
        "preact": false,
        "styled-components": false
      },
      "scopeHoist": false,
      "sourceMap": false,
      "engines": {
        "node": ">= 16"
      }
    }
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@parcel/reporter-bundle-analyzer": "^2.7.0",
    "babel-plugin-styled-components": "^2.0.7",
    "buffer": "^6.0.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "parcel": "^2.7.0",
    "parcel-resolver-shim": "link:./parcel-resolver-shim",
    "postcss": "^8.4.16",
    "prettier": "^2.4.1",
    "process": "^0.11.10"
  },
  "alias": {
    "app": "./app/src",
    "canvas": false,
    "react": "preact/compat",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime",
    "react/jsx-dev-runtime": "preact/compat/jsx-dev-runtime",
    "@mui/material/utils": "./app/src/alias"
  },
  "resolutions": {
    "prosemirror-model": "1.16.1"
  }
}
