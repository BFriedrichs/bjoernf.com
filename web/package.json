{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "homepage": "/public",
  "dependencies": {
    "@remirror/pm": "^1.0.22",
    "@remirror/react": "^1.0.43",
    "postinstall-postinstall": "^2.1.0",
    "preact": "^10.11.2",
    "react-router-dom": "^6.3.0",
    "react-share": "^4.4.0",
    "react-toast": "^1.0.3",
    "remirror": "^1.0.89"
  },
  "scripts": {
    "start": "parcel watch --target default --target node",
    "dev": "parcel --target dev --port 3000",
    "build": "parcel build --target default --target node",
    "build:report": "parcel build  --target default --target node --reporter @parcel/reporter-bundle-analyzer"
  },
  "targets": {
    "default": {
      "source": "app/src/index.html",
      "distDir": "dist/browser",
      "context": "browser",
      "publicUrl": "/static",
      "scopeHoist": true,
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    },
    "dev": {
      "source": "app/src/index.html",
      "distDir": "dist/dev",
      "context": "browser",
      "publicUrl": "./",
      "scopeHoist": true,
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    },
    "node": {
      "source": "app/src/AppServer.js",
      "distDir": "dist/node",
      "context": "node",
      "publicUrl": "/static",
      "outputFormat": "commonjs",
      "includeNodeModules": {
        "preact": false,
        "react": false,
        "react-dom": false,
        "styled-components": false
      },
      "scopeHoist": false,
      "sourceMap": false,
      "engines": {
        "node": ">= 16"
      }
    }
  },
  "devDependencies": {
    "@parcel/reporter-bundle-analyzer": "^2.7.0",
    "@parcel/transformer-inline-string": "2.7.0",
    "@parcel/transformer-sass": "2.7.0",
    "@parcel/transformer-svg-react": "^2.7.0",
    "babel-plugin-styled-components": "^2.0.7",
    "buffer": "^6.0.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "parcel": "^2.7.0",
    "parcel-resolver-shim": "link:./parcel-resolver-shim",
    "postcss": "^8.4.18",
    "prettier": "^2.4.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "process": "^0.11.10",
    "tailwindcss": "^3.2.1"
  },
  "alias": {
    "app": "./app/src",
    "canvas": false,
    "uglify-js": false,
    "react": "preact/compat",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime"
  },
  "resolutions": {
    "prosemirror-model": "1.16.1"
  }
}
