{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "homepage": "/public",
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.0.0",
    "@mui/material": "^5.0.0",
    "@mui/styles": "^5.0.0",
    "@nivo/chord": "^0.74.0",
    "@nivo/core": "^0.74.0",
    "@nivo/funnel": "^0.74.0",
    "@nivo/line": "^0.74.0",
    "@nivo/sankey": "^0.74.0",
    "@remirror/pm": "^1.0.22",
    "@remirror/react": "^1.0.43",
    "@swc/helpers": "^0.4.3",
    "date-fns": "^2.29.1",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.5",
    "prompts": "2.4.0",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^17.0.2",
    "react-refresh": "^0.8.3",
    "react-router-dom": "^5.3.0",
    "react-share": "^4.4.0",
    "regenerator-runtime": "^0.13.9",
    "remirror": "^1.0.89",
    "resolve": "1.18.1",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "parcel watch",
    "build": "rm -rf dist && parcel build"
  },
  "targets": {
    "default": {
      "source": "app/src/index.html",
      "distDir": "dist/browser",
      "context": "browser",
      "publicUrl": "/static",
      "scopeHoist": true,
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    },
    "node": {
      "source": "app/src/AppServer.js",
      "distDir": "dist/node",
      "context": "node",
      "publicUrl": "/static",
      "outputFormat": "commonjs",
      "includeNodeModules": {
        "react": false,
        "react-dom": false
      },
      "scopeHoist": false,
      "sourceMap": false,
      "engines": {
        "node": ">= 16"
      }
    }
  },
  "devDependencies": {
    "@parcel/reporter-bundle-analyzer": "^2.7.0",
    "buffer": "^6.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "parcel": "^2.7.0",
    "prettier": "^2.4.1",
    "process": "^0.11.10",
    "parcel-resolver-shim": "link:./parcel-resolver-shim"
  },
  "alias": {
    "app": "./app/src",
    "canvas": false
  },
  "resolutions": {
    "prosemirror-model": "1.16.1"
  }
}
